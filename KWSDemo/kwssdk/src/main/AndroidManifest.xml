<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kws.superawesome.tv.kwssdk">

    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <service android:name="kws.superawesome.tv.kwslib.SAAsyncTask$SAAsync" android:exported="false"/>

        <service
            android:name="kws.superawesome.tv.kwslib.SAStubMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>

        <service
            android:name="kws.superawesome.tv.kwssdk.push.KWSRegistrationService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

    </application>

</manifest>
